{
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "node": true
  },
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "error",
    "@typescript-eslint/no-var-requires": "off"
  }
}
